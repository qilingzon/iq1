---
import { t } from "@util/translate";
import { Icon } from "astro-icon/components";
import * as PageFinderModule from "./PageFind.vue";

const PageFinder = (PageFinderModule as { default: any }).default;
---

<PageFinder
  client:visible
  translations={{
    search: t("search"),
    close: t("close"),
    clear: t("clear"),
  }}
>
  <Icon name="search" class="h-6 w-6" slot="btn-search" />
  <Icon name="close" class="h-10 w-10" slot="btn-close" />
</PageFinder>
